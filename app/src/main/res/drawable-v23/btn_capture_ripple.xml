<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" android:color="@color/colorPD">
    <item>
        <layer-list >
            <item android:width="90dp" android:height="90dp">
                <shape android:shape="oval">
                    <solid android:color="@color/colorTransparent"/>
                    <stroke android:color="@color/colorWhiteText" android:width="1dp"/>
                </shape>
            </item>
            <item android:width="80dp" android:height="80dp" android:start="5.15dp" android:top="5.15dp">
                <shape android:shape="oval">
                    <solid android:color="@color/colorWhiteText"/>
                    <stroke android:color="@color/colorWhiteText" android:width="1dp"/>
                </shape>
            </item>
        </layer-list>
    </item>

</ripple>