<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="100dp" android:height="100dp">
        <shape android:shape="oval">
            <solid android:color="@color/colorTransparent"/>
            <stroke android:color="@color/colorWhiteText" android:width="1dp"/>
        </shape>
    </item>
    <item android:width="90dp" android:height="90dp" android:start="5dp" android:top="5dp">
        <shape android:shape="oval">
            <solid android:color="@color/colorWhiteText"/>
            <stroke android:color="@color/colorWhiteText" android:width="1dp"/>
        </shape>
    </item>
</layer-list>